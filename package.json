{
	"name": "modellplayer",
	"main": "main.js",
	"dependencies": {
		"coralian": "0.0.14",
		"node-xlsx": "0.15.0",
		"xlsx": "^0.15.0"
	},
	"scripts": {
		"start": "electron .",
		"debug": "electron . --debug-brk=5858",
		"test": "electron-packager . modellplayer --win --out ../ --arch=x64 --electronVersion 6.0.1 -overwrite=force --icon=./res/ico/16_16 --ignore=\"(test|dist|doc|.vscode|.gitignore|package-lock.json|README.md)\"",
		"publish": "electron-packager . modellplayer --win --out ../ --arch=x64 --asar --electronVersion 6.0.1 --overwrite=force --icon=./res/ico/16_16 --ignore=\"(test|dist|doc|.vscode|.gitignore|package-lock.json|README.md)\""
	}
}
